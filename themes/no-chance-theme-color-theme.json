{
  "name": "No Chance Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#202020",
    "editor.foreground": "#bbbbbb",
    "editor.findMatchBackground": "#00a4c8",
    "editor.selectionHighlightBorder": "#3d7e6d",
    "editor.selectionHighlightBackground": "#3d7e6d",
    "editor.findMatchBorder": "#00a3c827",
    "editorLineNumber.foreground": "#00a3c88a",
    "editorGroup.dropBackground": "#00a3c827",
    "editorGroup.border": "#00a3c827",
    "editorGroupHeader.border": "#00a3c827",
    "editorGroupHeader.tabsBorder": "#00a3c827",
    "focusBorder": "#00a4c8",
    "editorGroupHeader.tabsBackground": "#343534",
    "editorWidget.background": "#343534",

    "activityBar.background": "#202020",
    "activityBar.foreground": "#00a4c8",
    "activityBar.border": "#23788b27",
    "activityBarBadge.background": "#196292",

    "sideBarTitle.foreground": "#bbbbbb",
    "sideBarSectionHeader.foreground": "#00a4c8",
    "statusBar.debuggingBackground": "#3d7e6d",
    "statusBar.background": "#00a3c869",
    "statusBar.noFolderBackground": "#343534",
    "sideBarSectionHeader.border": "#00a4c8",
    "sideBar.border": "#00a3c827",

    "button.background": "#00a4c8",
    "button.foreground": "#EEFFFF",
    "button.hoverBackground": "#196292",

    "list.dropBackground": "#00a4c8",
    "list.activeSelectionBackground": "#00a3c8b0",
    "list.focusBackground": "#00a3c8b0",
    "list.hoverBackground": "#00a3c827",

    "panel.border": "#00a3c827",
    "panel.background": "#232323",
    "panelTitle.activeForeground": "#EEFFFF",

    "scrollbarSlider.background": "#084d8180",
    "scrollbarSlider.activeBackground": "#00a3c827",

    "gitDecoration.addedResourceForeground": "#00a3c8",
    "gitDecoration.modifiedResourceForeground": "#00a3c8b0",
    "gitDecoration.deletedResourceForeground": "#fc4c87dc",
    "gitDecoration.untrackedResourceForeground": "#91DACD",
    "gitDecoration.ignoredResourceForeground": "#e06e94",
    "gitDecoration.conflictingResourceForeground": "#82aaff",

    "tab.hoverBorder": "#00a4c8",
    "tab.activeBackground": "#00a3c849",
    "tab.activeBorderTop": "#00a4c8",
    "tab.inactiveBackground": "#031e2449",
    "tab.activeForeground": "#ffffff",

    "peekViewEditor.background": "#272727",
    "peekView.border": "#00a4c8",

    "terminal.border": "#00a4c8",
    "dropdown.background": "#343534",
    "menu.background": "#343534",
    "minimap.background": "#343534"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#546E7A"
      }
    },
    {
      "name": "String Quotes",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#BAB5F7"
      }
    },
    {
      "name": "Makefile and Markdown Text",
      "scope": [
        "source.makefile",
        "meta.scope.recipe.makefile",
        "fenced_code.block.language.markdown"
      ],
      "settings": {
        "foreground": "#F7ECB5"
      }
    },
    {
      "name": "Markdown Bold and Italic",
      "scope": ["markup.bold.markdown", "markup.italic.markdown"],
      "settings": {
        "foreground": "#91DACD"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "foreground": "#fc4c87dc"
      }
    },
    {
      "name": "Markdown Bold and Italic",
      "scope": ["markup.bold.markdown", "markup.italic.markdown"],
      "settings": {
        "foreground": "#91DACD"
      }
    },
    {
      "name": "Markdown Shellscript",
      "scope": ["meta.embedded.block.shellscript"],
      "settings": {
        "foreground": "#91DACD"
      }
    },
    {
      "name": "Makefile Func, Markdown Markup",
      "scope": [
        "entity.name.function.target.makefile",
        "meta.scope.target.makefile",
        "source.makefile"
      ],
      "settings": {
        "foreground": "#196292"
      }
    },
    {
      "name": "Variables and Colors",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "variable.other.constant",
        "meta.block variable.other",
        "support.variable.property",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword Storage, JSX tag and Sub-methods",
      "scope": [
        "storage.type",
        "storage.modifier",
        "entity.name.tag",
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js",
        "meta.export.default.tsx",
        "storage.modifier"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#fc4c87"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "meta.tag",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.type"
      ],
      "settings": {
        "foreground": "#25b1f3"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#49cce9"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link",
        "variable.parameter",
        "variable.other.object"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Number, Constant,Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.escape",
        "keyword.other",
        "constant.language.boolean.false"
      ],
      "settings": {
        "foreground": "#FF82B4"
      }
    },
    {
      "name": "Unit",
      "scope": ["keyword.other.unit", "support.constant"],
      "settings": {
        "foreground": "#EC9CD2"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading, Other Object Property",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "variable.other.object.property",
        "variable.other.object.property",
        "variable.other.object.property",
        "variable.other.object.property",
        "constant.language.boolean.true"
      ],
      "settings": {
        "foreground": "#91DACD"
      }
    },
    {
      "name": "Object Property, Literal and Binding Patterns",
      "scope": [
        "meta.object-literal.key",
        "meta.object-binding-pattern-variable",
        "variable.other.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#F7ECB5"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "meta.var.expr",
        "meta.field.declaration",
        "support.class",
        "markup.changed.git_gutter",
        "entity.other.inherited-class",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#83f0cc"
      }
    },
    {
      "name": "Type Parameters",
      "scope": ["meta.type.parameters"],
      "settings": {
        "fontStyle": "",
        "foreground": "#D5BDFF"
      }
    },
    {
      "name": "Entity and Primitive Types",
      "scope": ["support.type.primitive"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bab5f7"
      }
    },
    {
      "name": "Type and Interface",
      "scope": [
        "entity.name.type.interface",
        "entity.other.inherited-class",
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#7FDBCA"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#7FDBCA"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js", "meta.tag"],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Method Constructor",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#F7ECB5"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#F7ECB5"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#52E7E1"
      }
